{
  "name": "zeet-run-action",
  "version": "0.0.1",
  "private": true,
  "description": "Zeet Run Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "gen": "graphql-codegen --config codegen.yml",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeet-co/zeet-run-action.git"
  },
  "keywords": [
    "actions",
    "zeet",
    "deploy"
  ],
  "author": "Zeet",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "graphql": "^15.6.0",
    "graphql-request": "^3.5.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.1.1",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/fragment-matcher": "^3.1.0",
    "@graphql-codegen/typescript-graphql-files-modules": "^2.1.0",
    "@graphql-codegen/typescript-graphql-request": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^2.1.0",
    "@types/node": "^16.7.13",
    "@typescript-eslint/parser": "^4.29.3",
    "@vercel/ncc": "^0.29.2",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
